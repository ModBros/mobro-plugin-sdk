{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "ValueOptions",
  "type": "array",
  "items": {
    "oneOf": [
      {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "label": {
            "type": "string",
            "maxLength": 128
          },
          "value": {
            "type": "string",
            "pattern": "^[\\w-]+$",
            "maxLength": 128
          }
        },
        "required": [
          "label",
          "value"
        ]
      }
    ]
  }
}

